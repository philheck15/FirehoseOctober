<div class="my-wrapper">
<h1 class="custom-font1">My Pics</h1>

<% @pics.each do |pic| %>
<div class="span3 shower-curtain">
	<div class="box">
	<h2 class="custom-font2">
		Hour <%= pic.hour %>
	</h2>

	<div>
		<%= image_tag pic.image %>
	</div>

	<h3 class="custom-font3">
		I feel <%= pic.emotion %>
	</h3>

	<p class="custom-font4">
		I learned that<%= pic.learned %>
	</p>

	</div>
</div>

<% end %>